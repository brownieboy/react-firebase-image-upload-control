{
  "name": "react-firebase-image-upload-control",
  "version": "1.3.1",
  "description": "An image uploader control for Firebase Storage",
  "keywords": [
    "react-component",
    "react",
    "Firebase",
    "component",
    "uploader"
  ],
  "bugs": {
    "url": "https://github.com/brownieboy/react-firebase-image-upload-control/issues"
