  },
  "license": "MIT",
  "author": "Michael Brown <brownieboy@gmail.com>",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --config ./component/webpack.config.js --mode production && cp ./component/src/*.d.ts ./dist",
    "build:dev": "webpack --config ./component/webpack.config.js --mode development && cp ./component/src/*.d.ts ./dist",
    "start": "watch 'webpack --config ./component/webpack.config.js' ./component/src",
    "test": "jest --config ./component/jest.config.ts",
    "build:demo": "react-scripts build",
    "start:demo": "react-scripts start",
    "test:demo": "react-scripts test"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
